{{define "content"}}
{{template "header"}}

<div id="content">

	{{range .}}

	{{with .Useddata}}
	{{range .}}



	<div class="date">
		{{.Date}}
	</div>

	<div>
		<ul id ="box" class="box">
			{{range .MainPages}}
			<a href="http://daily.zhihu.com/story/{{.Id}}" >
				<li>
					{{.Title}}
				</li>
			</a>
			{{end}}
		</ul>
	</div>

	{{end}}

	{{end}}


	{{with .Pagemark}}
	<div class="pages">

		{{range .}}
		<a href="/page/{{.}}">{{.}}</a>
		{{end}}
	</div>

	{{end}}

	{{end}}

</div>
{{template "footer"}} 

{{end}}